{
  "name": "tinkerun",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "lerna run rebuild --scope app",
    "start:app": "lerna run start --scope app --stream",
    "start:web": "lerna run start --scope web --stream",
    "start": "run-p start:web start:app",
    "build:packages": "lerna run build",
    "build:electron": "electron-builder --config builder.js",
    "build": "run-s build:packages build:electron",
    "test": "lerna run test"
  },
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5"
  }
}
